using System;

namespace DodawanieLiczbCalkowitych
{
    class Program
    {
        static void Main(string[] args)
        {
            
            int sum = 0;
            for (int i = 0; i < 3; i++)
            {
                string s = Console.ReadLine();

                sum += int.Parse(s);
            }

            Console.WriteLine(sum);
        }
    }
}
