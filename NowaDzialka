using System;

namespace NowaDzia≈Çka
{
    class Program
    {
        static void Main(string[] args)
        {
            int t = int.Parse(Console.ReadLine());

            for (int i = 0; i < t; i++)
            {
                int k = int.Parse(Console.ReadLine());

                Console.WriteLine(k*k);
            }
        }
    }
}
